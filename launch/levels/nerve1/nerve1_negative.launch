<launch>
  <!-- Launch in either navigate mode or mapping mode -->
  <arg name="navigate" default="true" />

  <!-- Include spawn_world.launch and set it's arguments -->
  <include file="$(find uml_3d_race)/launch/spawn_world.launch">
    <!-- World file -->
    <arg name="world_name" value="nerve1_negative" />
    <arg name="world_path" value="nerve1/nerve1_negative.world" />
    <!-- Robot spawn position -->
    <arg name="spawn_x" value="0.0" />
    <arg name="spawn_y" value="4.0" />
    <arg name="spawn_z" value="0.2" />
    <arg name="spawn_yaw" value="0.0" />

    <arg name="goal_x" value="0.0" />
    <arg name="goal_y" value="8.0" />

    <arg name="navigate" value="$(arg navigate)" />
  </include>
</launch>
